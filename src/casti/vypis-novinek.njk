<ul class="vypis-novinek">
	{% for zprava in novinky %}
		<li class="vypis-novinek__polozka">
			<a href="/zprava/{{ zprava.titulek | slug }}">
				{{ zprava.titulek }}
				(<time class="vypis-novinek__datum">
					{{ zprava.datum.den }}.
					{{ zprava.datum.mesic }}.
				</time>)
			</a>
		</li>
	{% endfor %}
</ul>
